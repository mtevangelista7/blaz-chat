@inherits ConfirmDialogBase

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h6">@Message</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Close">Sim</MudButton>
        <MudButton Color="Color.Error" OnClick="Cancel">Não</MudButton>
    </DialogActions>
</MudDialog>